# API Keys (replace with your actual keys)
VITE_TCGPLAYER_API_KEY=your_tcgplayer_key_here
VITE_TCGPLAYER_PUBLIC_KEY=your_public_key_here
VITE_TCGPLAYER_PRIVATE_KEY=your_private_key_here

# eBay API
VITE_EBAY_APP_ID=your_ebay_app_id
